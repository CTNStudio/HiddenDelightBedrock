{
  "include": ["src/*", "src/core/utils.ts"],
  "compilerOptions": {
    "target": "es2022",
    "module": "ES2022",
    "moduleResolution": "node10",
    //"declaration": true,
    "outDir": "./tsc-out",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  }
}
